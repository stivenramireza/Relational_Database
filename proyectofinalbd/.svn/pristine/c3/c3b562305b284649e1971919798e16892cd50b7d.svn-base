    private void listaJugadorActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_listaJugadorActionPerformed
        String list = (String) listaJugador.getSelectedItem();
        String query1 = "SELECT idJugador, nombreJugador, pesoJugador, estaturaJugador, nombrePosicionJugador, fotoJugador "
                + "FROM laliga.Jugador INNER JOIN laliga.PosicionJugador ON "
                + "(Jugador.PosicionJugador_idPosicionJugador = PosicionJugador.idPosicionJugador) WHERE nombreJugador = '" +list+"';";
        try{
            PreparedStatement pst1 = connection.prepareStatement(query1);
            rt1 = pst1.executeQuery();
            String item1 = "...Seleccionar jugador...";
            if(!listaJugador.getSelectedItem().equals(item1)){
            if(rt1.next()){
            txtIDJugador.setText(rt1.getString(1));
            txtNombreJugador.setText(rt1.getString(2));
            txtPesoJugador.setText(rt1.getString(3));
            txtEstaturaJugador.setText(rt1.getString(4));
            listaPosicion.setSelectedItem(rt1.getString(5));
            lblImagenJugador.setIcon(new ImageIcon(getClass().getResource("/Imagenes/" + rt1.getString(6))));
            }
            }else{
                limpiar();
            }
        }catch(Exception e){
             JOptionPane.showMessageDialog(null, e.getMessage(), "Error en la conexion con la BD",
                    JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_listaJugadorActionPerformed

