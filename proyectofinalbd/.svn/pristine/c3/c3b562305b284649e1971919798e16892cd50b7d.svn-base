    private void btnRegistrarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnRegistrarActionPerformed
        String querie = "INSERT INTO laliga.Jugador (idJugador, nombreJugador, pesoJugador, estaturaJugador, "
                      + "PosicionJugador_idPosicionJugador, Club_idClub, jugadorActivo, fotoJugador) VALUES (?,?,?,?,?,?,?,?);";
        try{
            PreparedStatement pst = connection.prepareStatement(querie);
            pst.setInt(1,Integer.parseInt(txtIDJugador.getText()));
            pst.setString(2,txtNombreJugador.getText());
            pst.setInt(3,Integer.parseInt(txtPesoJugador.getText()));
            pst.setInt(4,Integer.parseInt(txtEstaturaJugador.getText()));
            pst.setInt(5, listaPosicion.getSelectedIndex());
            pst.setInt(6, Integer.parseInt(lblIDClub.getText()));
            pst.setInt(7, 1);
            pst.setString(8,result);
            pst.executeUpdate();
            JOptionPane.showMessageDialog(null, "Se  ha registrado un nuevo jugador \n","Insertar nuevo",JOptionPane.QUESTION_MESSAGE);
        }catch(Exception e){
             JOptionPane.showMessageDialog(null, e.getMessage(), "Error en la conexion con la BD",
                    JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_btnRegistrarActionPerformed

