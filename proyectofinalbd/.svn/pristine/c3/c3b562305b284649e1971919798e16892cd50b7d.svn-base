    private void btnEliminarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnEliminarActionPerformed
        dispose();
        int noActivo = 0;
        int ax = JOptionPane.showConfirmDialog(null, "Â¿Estas seguro que deseas eliminar a este jugador?",null, JOptionPane.YES_NO_OPTION);
