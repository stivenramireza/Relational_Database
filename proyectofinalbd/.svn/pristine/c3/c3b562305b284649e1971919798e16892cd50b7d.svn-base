                        "estaturaJugador = '"+txtEstaturaJugador.getText()+"', " +
                        "PosicionJugador_idPosicionJugador = '"+listaPosicion.getSelectedIndex()+"', " +
                        "Club_idClub = '"+lblIDClub.getText()+"', " +
                        "jugadorActivo = " +noActivo+"," +
                        "fotoJugador '"+result+"' " +
                        "WHERE idJugador = "+Integer.parseInt(txtIDJugador.getText())+";";
        try{
